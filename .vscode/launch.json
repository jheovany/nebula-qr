{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Go Main",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/main.go",
      "cwd": "${workspaceFolder}",
      "env": {},
      "args": []
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Open Swagger in Chrome",
      "url": "http://localhost:8080/swagger/index.html",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
